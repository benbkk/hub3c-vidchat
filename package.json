{
  "name": "hub3c-vidchat",
  "version": "1.0.0",
  "description": "Hub3C video chat app prototype by Ben Anggoro for Geekseat",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/benworkandplay/hub3c-vidchat"
  },
  "homepage": "http://geekseat.com.au",
  "main": "Gulpfile.js",
  "scripts": {
    "test": "gulp"
  },
  "author": "Ben Anggoro <ben@benworkandplay.com> (http://www.benworkandplay.com)",
  "license": "UNLICENSED",
  "paths": {
    "tasks": "gulp-tasks/",
    "src": "project/",
    "build": "_dist/",
    "bower": "bower_components/",
    "vendor": {
      "src": "project/js/lib/",
      "dest": "_dist/js/"
    },
    "js": {
      "src": "project/js/",
      "dest": "_dist/js/"
    },
    "compass": "compass-mixins/lib/",
    "bootstrap": {
      "sass": "bootstrap-sass/assets/stylesheets/",
      "js": "bootstrap-sass/assets/javascripts/",
      "fonts": "bootstrap-sass/assets/fonts/"
    },
    "fontawesome": "font-awesome/scss/",
    "sass": {
      "src": "project/_sass/"
    },
    "hamburgers": "css-hamburgers/_sass/hamburgers/",
    "css": {
      "src": "project/css/",
      "dest": "_dist/css/"
    },
    "img": {
      "src": "project/img/",
      "dest": "_dist/img/"
    }
  },
  "dependencies": {
    "graceful-fs": "^4.1.4",
    "gulp": "^3.9.0",
    "gulp-auto-task": "0.0.1",
