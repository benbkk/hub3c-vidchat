    "gulp-imagemin": "^2.3.0",
    "gulp-jshint": "^1.11.2",
    "gulp-rename": "^1.2.2",
    "gulp-ruby-sass": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.4",
    "imagemin-pngquant": "^4.1.2",
    "lodash": "^4.13.1",
    "minimatch": "^3.0.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.2"
  },
  "devDependencies": {
    "browser-sync": "^2.8.2",
    "graceful-fs": "^4.1.4",
    "gulp-clean-css": "^2.0.12",
    "gulp-filter": "^4.0.0",
    "gulp-main-bower-files": "^1.5.2",
    "gulp-plumber": "^1.1.0",
